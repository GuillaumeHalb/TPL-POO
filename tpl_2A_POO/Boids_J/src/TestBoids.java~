import gui.GUISimulator;
import java.awt.Color;

public class TestBoids {
    public static void main(String[] args) {

	EssaimSimulator E = new EssaimSimulator(50,50,6,2);
        //C.getCells().reInit();
	for(i=1;i<6;i++){
	    Pt Pos = new Pt(10.0*i,10.0*i);
	    Pt dir = new Pt(1.0,1.0);
	    Individu ind = new Individu (1,Pos,dir);
	    E.ajouteIndividu(ind);
	}
	C.getGUI().setSimulable(E);
    }
}
